{
  "name": "nodebb-plugin-dateintitle",
  "version": "0.1.0",
  "description": "Puts the date and location in the title of an event (to be used with the calender plugin)",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/floydremmerswaal/nodebb-plugin-dateintitle"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "dateintitle",
    "shell"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": {
    "name": "Floyd Remmerswaal",
    "email": "floydremmerswaal@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/floydremmerswaal/nodebb-plugin-dateintitle/issues"
  },
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^1.15.0"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-angular": "15.0.0",
    "eslint": "8.4.1",
    "eslint-config-nodebb": "0.1.1",
    "eslint-plugin-import": "2.25.3",
    "husky": "7.0.4",
    "lint-staged": "12.1.2"
  }
}
